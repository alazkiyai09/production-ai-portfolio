n",
    "print(\"ðŸ“Š Campaign Visualization\")\n",
    "\n",
    "# Daily performance comparison\n",
    "daily = df.groupby(['date', 'campaign']).agg({\n",
    "    'conversions': 'sum',\n",
    "    'cost': 'sum'\n",
    "}).reset_index()\n",
    "\n",
    "fig = px.line(daily, x='date', y='conversions', color='campaign',\n",
    "              title='Daily Conversions by Campaign', markers=True)\n",
    "fig.show()\n",
    "\n",
    "# ROI comparison\n",
    "roi_df = df.groupby('campaign')['roi'].sum().reset_index()\n",
    "fig2 = px.bar(roi_df, x='campaign', y='roi', \n",
    "              title='ROI by Campaign', color='campaign')\n",
    "fig2.show()"
