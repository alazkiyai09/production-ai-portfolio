   "source": [
    "## 2. Agent Tools"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": null,
   "metadata": {},
   "outputs": [],
   "source": [
    "import pandas as pd\n",
    "import plotly.express as px\n",
    "import numpy as np\n",
    "\n",
    "# Sample campaign data\n",
    "campaign_data = {\n",
    "    'date': pd.date_range('2024-01-01', periods=30),\n",
    "    'campaign': ['A'] * 15 + ['B'] * 15,\n",
    "    'impressions': np.random.randint(1000, 10000, 30),\n",
    "    'clicks': np.random.randint(100, 1000, 30),\n",
    "    'conversions': np.random.randint(10, 100, 30),\n",
    "    'cost': np.random.uniform(100, 500, 30)\n",
    "}\n",
    "\n",
    "df = pd.DataFrame(campaign_data)\n",
    "df['ctr'] = df['clicks'] / df['impressions']\n",
    "df['conversion_rate'] = df['conversions'] / df['clicks']\n",
    "df['roi'] = (df['conversions'] * 50) - df['cost']  # Assuming $50 per conversion\n",
    "\n",
    "print(\"ðŸ“Š Campaign Data Created\")\n",
    "print(f\"  â€¢ Records: {len(df)} days\")\n",
    "print(f\"  â€¢ Campaigns: {df['campaign'].unique()}\")\n",
    "print(f\"  â€¢ Total Cost: ${df['cost'].sum():.2f}\")\n",
    "print(f\"  â€¢ Total Conversions: {df['conversions'].sum()}\")"
   ]
  },
  {
   "cell_type": "markdown",
   "metadata": {},
   "source": [
    "## 3. Agent Analysis Workflow"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": null,
   "metadata": {},
   "outputs": [],
   "source": [
    "def analyze_campaign_performance(df: pd.DataFrame, campaign: str) -> dict:\n",
    "    \"\"\"Analyze campaign performance.\"\"\"\n",
    "    camp_df = df[df['campaign'] == campaign]\n",
    "    \n",
    "    return {\n",
    "        'total_impressions': camp_df['impressions'].sum(),\n",
    "        'total_clicks': camp_df['clicks'].sum(),\n",
    "        'total_conversions': camp_df['conversions'].sum(),\n",
    "        'total_cost': camp_df['cost'].sum(),\n",
    "        'avg_ctr': camp_df['ctr'].mean(),\n",
    "        'avg_conversion_rate': camp_df['conversion_rate'].mean(),\n",
    "        'total_roi': camp_df['roi'].sum()\n",
    "    }\n",
    "\n",
    "# Analyze both campaigns\n",
    "print(\"ðŸ“ˆ Campaign Analysis\\n\")\n",
    "print(\"=\" * 60)\n",
    "\n",
    "for campaign in ['A', 'B']:\n",
    "    metrics = analyze_campaign_performance(df, campaign)\n",
    "    print(f\"\\nðŸ“¢ Campaign {campaign}:\")\n",
    "    print(f\"  Impressions: {metrics['total_impressions']:,}\")\n",
    "    print(f\"  Clicks: {metrics['total_clicks']:,}\")\n",
    "    print(f\"  Conversions: {metrics['total_conversions']}\")\n",
    "    print(f\"  Cost: ${metrics['total_cost']:.2f}\")\n",
    "    print(f\"  CTR: {metrics['avg_ctr']:.2%}\")\n",
    "    print(f\"  Conv. Rate: {metrics['avg_conversion_rate']:.2%}\")\n",
    "    print(f\"  ROI: ${metrics['total_roi']:.2f}\")"
   ]
  },
  {
   "cell_type": "markdown",
   "metadata": {},
   "source": [
