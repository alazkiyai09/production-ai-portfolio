    "## 7. RAG-Enhanced Query Context"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": null,
   "metadata": {},
   "outputs": [],
   "source": [
    "def rag_enhanced_query(query: str, kb: list) -> str:\n",
    "    \"\"\"Enhanced query with RAG context.\"\"\"\n",
    "    # Check knowledge base for relevant context\n",
    "    relevant_docs = [doc for doc in kb if any(keyword in query.lower() for keyword in doc['answer'].lower().split())]\n",
    "    \n",
    "    context = \"\"\n",
    "    if relevant_docs:\n",
    "        context = \"Context: \" + \" | \".join([d['answer'][:100] for d in relevant_docs])\n",
    "    \n",
    "    # Generate SQL\n",
    "    sql = nl_to_sql(query, {})\n",
    "    \n",
    "    # Execute\n",
    "    if 'GROUP BY product' in sql:\n",
    "        result = sales_df.groupby('product')['amount'].sum().reset_index()\n",
    "    elif 'GROUP BY region' in sql:\n",
    "        result = sales_df.groupby('region')['amount'].sum().reset_index()\n",
    "    else:\n",
    "        result = sales_df.head(5)\n",
    "    \n",
    "    return f\"\"\"{context}\n",
    "\\n\",\n",
    "Query Result:\n",
    "{result.to_string(index=False)}\\n",
    "\n",
    "Recommended Visualization: {result['viz_type']}\n",
    "\"\"\"\n",
    "\n",
    "# Test RAG-enhanced query\n",
    "complex_query = \"How do I compare product sales?\"\n",
    "\n",
    "print(\"ðŸ¤– RAG-Enhanced Analytics\\n\")\n",
    "print(\"=\" * 70)\n",
    "\n",
    "response = rag_enhanced_query(complex_query, kb_docs)\n",
    "print(response)"
   ]
  },
