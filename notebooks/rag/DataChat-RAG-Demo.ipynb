    "## 3. Natural Language to SQL"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": null,
   "metadata": {},
   "outputs": [],
   "source": [
    "def nl_to_sql(query: str, schema: dict) -> str:\n",
    "    \"\"\"\n",
    "    Convert natural language to SQL (simplified).\n",
    "    \n",
    "    In production, this uses an LLM. For demo, we use pattern matching.\n",
    "    \"\"\"\n",
    "    query_lower = query.lower()\n",
    "    \n",
    "    # Pattern matching for common queries\n",
    "    if 'total sales' in query_lower or 'sum of sales' in query_lower:\n",
    "        return \"SELECT SUM(amount) as total_sales FROM sales;\"\n",
    "    elif 'average' in query_lower:\n",
    "        return \"SELECT AVG(amount) as avg_sales FROM sales;\"\n",
    "    elif 'top product' in query_lower or 'best selling' in query_lower:\n",
    "        return \"SELECT product, SUM(amount) as total FROM sales GROUP BY product ORDER BY total DESC LIMIT 1;\"\n",
    "    elif 'sales by region' in query_lower:\n",
    "        return \"SELECT region, SUM(amount) as total FROM sales GROUP BY region;\"\n",
    "    elif 'sales by product' in query_lower:\n",
    "        return \"SELECT product, SUM(amount) as total FROM sales GROUP BY product;\"\n",
    "    else:\n",
    "        # Default: all sales\n",
    "        return \"SELECT * FROM sales LIMIT 10;\"\n",
    "\n",
    "# Test NL to SQL conversion\n",
    "test_queries = [\n",
    "    \"What are the total sales?\",\n",
    "    \"Show me sales by region\",\n",
    "    \"What's the average sale amount?\",\n",
    "    \"Which product sells the most?\",\n",
    "    \"Show sales by product\"\n",
    "]\n",
    "\n",
    "print(\"ðŸ’¬ Natural Language to SQL\\n\")\n",
    "print(\"=\" * 70)\n",
    "\n",
    "for query in test_queries:\n",
    "    sql = nl_to_sql(query, {})\n",
    "    print(f\"\\nQ: {query}\")\n",
    "    print(f\"SQL: {sql}\")\n",
    "    \n",
    "    # Execute query\n",
    "    result = sales_df.query(sql.replace('SELECT ', '').replace(' FROM sales', '').replace(';', ''))\n",
    "    print(f\"Result: {result.to_dict('records')[:2]}\")"
   ]
  },
