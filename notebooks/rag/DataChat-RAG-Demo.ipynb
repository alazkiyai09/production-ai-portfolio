   "source": [
    "## 6. Visualization Generation"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": null,
   "metadata": {},
   "outputs": [],
   "source": [
    "# Generate visualizations\n",
    "print(\"ðŸ“Š Visualization Generation\\n\")\n",
    "\n",
    "# Sales by product (bar chart)\n",
    "product_sales = sales_df.groupby('product')['amount'].sum().reset_index()\n",
    "fig1 = px.bar(product_sales, x='product', y='amount', \n",
    "             title='Sales by Product', color='product')\n",
    "print(\"1. Bar chart: Sales by product\")\n",
    "fig1.show()\n",
    "\n",
    "# Sales by region (pie chart)\n",
    "region_sales = sales_df.groupby('region')['amount'].sum().reset_index()\n",
    "fig2 = px.pie(region_sales, values='amount', names='region',\n",
    "             title='Sales Distribution by Region')\n",
    "print(\"\\n2. Pie chart: Sales by region\")\n",
    "fig2.show()\n",
    "\n",
    "# Time series\n",
    "daily_sales = sales_df.groupby('date')['amount'].sum().reset_index()\n",
    "fig3 = px.line(daily_sales, x='date', y='amount', \n",
    "             title='Daily Sales Over Time')\n",
    "print(\"\\n3. Line chart: Daily sales trend\")\n",
    "fig3.show()"
   ]
